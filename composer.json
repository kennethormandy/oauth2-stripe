{
    "name": "adam-paterson/oauth2-stripe",
    "description": "Stripe OAuth 2.0 Client Provider for The PHP League OAuth2-Client, including urlAuthorize support for Stripe Connect.",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "stripe",
        "stripe api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Paterson",
            "email": "hello@adampaterson.co.uk"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0",
        "scrutinizer/ocular": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "AdamPaterson\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdamPaterson\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "scripts": {
      "format": "vendor/bin/phpcs --standard=psr2 src/",
      "test": "phpunit --coverage-text --coverage-clover=coverage.clover"
    }
}
